{
  "name": "frolf-micro",
  "version": "0.0.0-development",
  "description": "Micro libary test",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "codecov -t 23a6d16e-b329-4fbd-893c-f205472d57e5",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x **/*.test.js node_modules/mocha/bin/_mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stibay/Frolf-micro.git"
  },
  "keywords": [
    "micro",
    "libary",
    "test"
  ],
  "author": "Stian Bay <stian.bay@outlook.com> (https://frolf.live/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stibay/Frolf-micro/issues"
  },
  "homepage": "https://github.com/stibay/Frolf-micro#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "4.0.1",
    "semantic-release": "^8.2.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
